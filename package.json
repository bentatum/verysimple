{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish-packages": "turbo run build lint && changeset version && changeset publish --access public"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "tsconfig": "*",
    "turbo": "latest"
  },
  "name": "verysimple",
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@changesets/get-dependents-graph": "^1.3.6",
    "@changesets/get-release-plan": "^3.0.17",
    "@changesets/parse": "^0.3.16"
  }
}
